FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 8.44;

boundaryField
{
    inlet
    {
       type            fixedValue;
       value           $internalField;
    }
    outlet
    {
        type            zeroGradient;
    }

    ground
    {
        type            omegaWallFunction;
        value           $internalField;
    }

    estate_walls
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    walls
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    symmetry
    {
        type             symmetry;
    }
}


// ************************************************************************* //
